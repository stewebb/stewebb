// Portfolio Modal 3
#portfolioModal3.portfolio-modal.modal.fade(tabindex='-1', aria-labelledby='portfolioModal3', aria-hidden='true')
    .modal-dialog.modal-xl
        .modal-content
            .modal-header.border-0
                button.btn-close(type='button', data-bs-dismiss='modal', aria-label='Close')
            .modal-body.text-center.pb-5
                .container
                    .row.justify-content-center
                        .col-lg-8
                            // Portfolio Modal - Title
                            h2.portfolio-modal-title.text-secondary.mb-0 Internship Project

                            // Icon Divider
                            .divider-custom
                                .divider-custom-line
                                .divider-custom-icon
                                    i.fas.fa-code
                                .divider-custom-line

                            // Portfolio Modal - Image Carousel
                            #portfolioModalCarouse3.carousel.slide(data-bs-ride='carousel').mb-3
                                
                                // Indicators/dots
                                .carousel-indicators
                                    button(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide-to='0', class='active', aria-current='true', aria-label='Slide 1')
                                    button(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide-to='1', aria-label='Slide 2')
                                    button(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide-to='2', aria-label='Slide 3')
                                    button(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide-to='3', aria-label='Slide 4')
                                    button(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide-to='4', aria-label='Slide 5')
                                    button(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide-to='5', aria-label='Slide 6')
                                    button(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide-to='6', aria-label='Slide 7')

                                // The slideshow/carousel
                                .carousel-inner
                                    .carousel-item.active
                                        img.d-block.w-100.img-fluid.rounded.mb-5(src='assets/img/screenshots/internship-1.png', alt='Home Page')
                                        .carousel-caption.caption
                                            h5.fw-bold Home Page
                                    .carousel-item
                                        img.d-block.w-100.img-fluid.rounded.mb-5(src='assets/img/screenshots/internship-2.png', alt='Form Validation')
                                        .carousel-caption.caption
                                            h5.fw-bold Form Validation
                                    .carousel-item
                                        img.d-block.w-100.img-fluid.rounded.mb-5(src='assets/img/screenshots/internship-3.png', alt='User QuickSelect')
                                        .carousel-caption.caption
                                            h5.fw-bold User QuickSelect
                                    .carousel-item
                                        img.d-block.w-100.img-fluid.rounded.mb-5(src='assets/img/screenshots/internship-4.png', alt='Rich-text Editor')
                                        .carousel-caption.caption
                                            h5.fw-bold Rich-text Editor
                                    .carousel-item
                                        img.d-block.w-100.img-fluid.rounded.mb-5(src='assets/img/screenshots/internship-5.png', alt='Budget Calculation')
                                        .carousel-caption.caption
                                            h5.fw-bold Budget Calculation
                                    .carousel-item
                                        img.d-block.w-100.img-fluid.rounded.mb-5(src='assets/img/screenshots/internship-6.png', alt='Risk Assessment')
                                        .carousel-caption.caption
                                            h5.fw-bold Risk Assessment
                                    .carousel-item    
                                        img.d-block.w-100.img-fluid.rounded.mb-5(src='assets/img/screenshots/internship-7.png', alt='Submission Email')
                                        .carousel-caption.caption
                                            h5.fw-bold Submission Email

                                // Left and right controls/icons
                                button.carousel-control-prev(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide='prev')
                                    span.carousel-control-prev-icon(aria-hidden='true').caption
                                    span.visually-hidden Previous
                                button.carousel-control-next(type='button', data-bs-target='#portfolioModalCarouse3', data-bs-slide='next')
                                    span.carousel-control-next-icon(aria-hidden='true').caption
                                    span.visually-hidden Next

                            // Portfolio Modal - Text
                            .text-start
                                p.mb-0.fw-bold 
                                    | Description
                                p.mb-2
                                    | This is a digital event management form.
                                //p.mb-0.fw-bold 
                                //    | Features  
                                //p.mb-2
                                //    | It includes features such as
                                //    span.fw-bold Event Calendar
                                //    | ,
                                //    span.fw-bold Banner Request/Approval
                                //    | , and
                                //    span.fw-bold Graphics Management
                                //    | . eTWIG is a free software licensed under the MIT License.
                                p.mb-1.fw-bold 
                                    | Tech Stack
                                p.mb-4 
                                    img.mr-1(src="assets/img/devicons/html5-original.svg" height="40" alt="HTML5")
                                    img.mr-1(src="assets/img/devicons/css3-original.svg" height="40" alt="CSS3")
                                    img.mr-1(src="assets/img/devicons/javascript-original.svg" height="40" alt="Javascript")
                                    img.mr-1(src="assets/img/devicons/PowerApps_scalable.svg" height="40" alt="PowerApps")
                                    img.mr-1(src="assets/img/devicons/PowerAutomate_scalable.svg" height="40" alt="Power Automate")
                                    //img.mr-1(src="assets/img/devicons/jquery-original.svg" height="40" alt="jQuery")
